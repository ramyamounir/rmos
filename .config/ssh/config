Match host * exec "gpg-connect-agent --quiet updatestartuptty /bye"

Host *
    ControlPath ~/.ssh/multiplex/%r@%h:%p
    ControlMaster auto
    ControlPersist 1
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    ForwardX11 yes
    ForwardX11Trusted yes

